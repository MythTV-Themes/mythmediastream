<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mythuitheme SYSTEM "http://www.mythtv.org/schema/mythuitheme.dtd">
<mythuitheme>

    <window name="osd_message">
        <fontdef name="medium" face="MediaStreamALT">
            <pixelsize>28</pixelsize>
            <stretch>extracondensed</stretch>
            <weight>normal</weight>
            <color>#FFFFFF</color>
            <shadowoffset>1,1</shadowoffset>
            <shadowcolor>#000000</shadowcolor>
        </fontdef>
        <area>465,0,350,70</area>
        <imagetype name="osd_messageBackground">
            <area>0,0,350,70</area>
            <filename>images/shared/InfoMessagePanel.png</filename>
        </imagetype>
        <textarea name="message_text">
            <font>medium</font>
            <case>upper</case>
            <area>10,5,330,60</area>
            <align>hcenter,vcenter</align>
            <multiline>yes</multiline>
            <cutdown>yes</cutdown>
        </textarea>
    </window>

    <window name="osd_input">
        <fontdef name="normal" face="MediaStreamALT">
            <pixelsize>28</pixelsize>
            <stretch>extracondensed</stretch>
            <weight>normal</weight>
            <color>#FFFFFF</color>
            <shadowoffset>1,1</shadowoffset>
            <shadowcolor>#000000</shadowcolor>
        </fontdef>
        <area>465,0,350,70</area>
        <imagetype name="osd_messageBackground">
            <area>0,0,350,70</area>
            <filename>images/shared/InfoMessagePanel.png</filename>
        </imagetype>
        <textarea name="osd_number_entry">
            <font>normal</font>
            <area>10,5,330,60</area>
            <align>hcenter,vcenter</align>
            <multiline>no</multiline>
            <cutdown>yes</cutdown>
            <template>Switch to Channel: %1</template>
        </textarea>
    </window>


    <window name="osd_status">
        <fontdef name="small" face="MediaStreamALT">
            <pixelsize>21</pixelsize>
            <color>#FFFFFF</color>
            <shadowoffset>1,1</shadowoffset>
            <shadowcolor>#000000</shadowcolor>
        </fontdef>
        <fontdef name="smaller" from="small">
            <pixelsize>17</pixelsize>
        </fontdef>
        <fontdef name="medium" from="small">
            <pixelsize>23</pixelsize>
        </fontdef>
        <area>0,500,1280,220</area>
        <imagetype name="popup_background">
            <area>0,0,1280,225</area>	
            <filename>images/shared/OSDBackground.png</filename>
        </imagetype>
        <textarea name="title">
            <font>medium</font>
            <area>10,40,600,30</area>
            <align>left,top</align>
        </textarea>
        <clock name="clock">
            <area>970,40,300,30</area>
            <font>small</font>
            <template>%TIME%</template>
            <align>right,top</align>
        </clock>

        <imagetype name="OSDPrevTrackNF">
            <area>436,70,50,50</area>	
            <filename>images/shared/OSDPrevTrackNF.png</filename>
            <preserveaspect>no</preserveaspect>
        </imagetype>
        <imagetype name="OSDRewindNF">
            <area>496,70,50,50</area>	
            <filename>images/shared/OSDRewindNF.png</filename>
            <preserveaspect>no</preserveaspect>
        </imagetype>
        <imagetype name="OSDStopNF" from="OSDRewindNF">
            <position>556,70</position>	
            <filename>images/shared/OSDStopNF.png</filename>
        </imagetype>
        <imagetype name="OSDPlayNF" from="OSDRewindNF">
            <position>616,70</position>	
            <filename>images/shared/OSDPlayNF.png</filename>
        </imagetype>
        <imagetype name="OSDPauseNF" from="OSDRewindNF">
            <position>676,70</position>	
            <filename>images/shared/OSDPauseNF.png</filename>
        </imagetype>
        <imagetype name="OSDForwardNF" from="OSDRewindNF">
            <position>736,70</position>	
            <filename>images/shared/OSDForwardNF.png</filename>
        </imagetype>
        <imagetype name="OSDNextTrackNF" from="OSDRewindNF">
            <position>796,70</position>	
            <filename>images/shared/OSDNextTrackNF.png</filename>
        </imagetype>

        <imagetype name="OSDBookmarksNF" from="OSDRewindNF">
            <position>1036,70</position>	
            <filename>images/shared/OSDBookmarksNF.png</filename>
        </imagetype>
        <imagetype name="OSDPlaylistNF" from="OSDRewindNF">
            <position>1096,70</position>	
            <filename>images/shared/OSDPlaylistNF.png</filename>
        </imagetype>

        <statetype name="bookmark">
            <state name="yes">
                <imagetype name="OSDBookmarksFO">
                    <position>1036,70</position>
                    <preserveaspect>no</preserveaspect>
                    <filename>images/shared/OSDBookmarksFO.png</filename>
                </imagetype>
            </state>
        </statetype>
        <statetype name="playlist">
            <state name="yes">
