    <window name="ChannelEditor">
        <fontdef name="small" face="MediaStreamALT">
            <pixelsize>23</pixelsize>
            <color>#FFFFFF</color>
            <shadowoffset>1,1</shadowoffset>
            <shadowcolor>#000000</shadowcolor>
        </fontdef>
        <fontdef name="smaller" from="small">
            <pixelsize>17</pixelsize>
        </fontdef>
        <area>390,110,500,500</area>
        <shape name="background">
            <area>0,0,100%,100%</area>
            <type>roundbox</type>
            <fill color="#000000" alpha="200" />
            <line color="#222222" alpha="255" width="2" />
            <cornerradius>12</cornerradius>
        </shape>
        <textarea name="title">
            <font>small</font>
            <area>15,15,470,150</area>
            <align>allcenter</align>
            <value>Channel Editor</value>
        </textarea>

        <textarea name="callsign_label">
            <font>small</font>
            <area>10,180,130,50</area>
            <align>allcenter</align>
            <value>Callsign</value>
        </textarea>
        <textedit name="callsign">
            <area>150,180,340,50</area>
            <statetype name="background">
                <state name="active">
                    <shape name="textbackground">
                        <fill color="#BBBBBB" alpha="255" />
                    </shape>
                </state>
                <state name="selected" from="active">
                    <shape name="textbackground">
                        <fill color="#BBBBBB" alpha="200" />
                    </shape>
                </state>
                <state name="inactive" from="active" />
            </statetype>
            <imagetype name="cursor">
                <filename>cursor.png</filename>
            </imagetype>
            <textarea name="text">
                <font>small</font>
                <align>left,vcenter</align>
            </textarea>
        </textedit>

        <textarea name="channum_label" from="callsign_label">
            <area>10,250,130,50</area>
            <value>Channel #</value>
        </textarea>
        <textedit name="channum" from="callsign">
            <area>150,250,340,50</area>
        </textedit>

        <textarea name="channame_label" from="callsign_label">
            <area>10,320,130,50</area>
            <value>Channel Name</value>
        </textarea>
        <textedit name="channame" from="callsign">
            <area>150,320,340,50</area>
        </textedit>

        <textarea name="xmltv_label" from="callsign_label">
            <area>10,390,130,50</area>
            <value>XMLTV ID</value>
        </textarea>
        <textedit name="XMLTV" from="callsign">
            <area>150,390,340,50</area>
        </textedit>

        <button name="probe">
