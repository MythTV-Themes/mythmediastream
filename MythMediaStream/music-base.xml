                    <filename>images/shared/OSDRewindFO.png</filename>
                </imagetype>
            </state>
            <state name="disabled" from="active" />
            <state name="pushed" from="active">
                <imagetype name="background">
                    <filename>images/shared/OSDRewindFO.png</filename>
                </imagetype>
            </state>
        </statetype>
    </button>

    <!-- pause button -->
    <button name="basepausebutton">
